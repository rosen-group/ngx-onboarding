{
  "name": "ngx-onboarding-app",
  "version": "1.9.1",
  "license": "MIT",
  "author": "ROSEN Group",
  "keywords": [
    "ngx-onboarding",
    "angular-onboarding",
    "onboarding"
  ],
  "scripts": {
    "ng": "ng",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "start": "ng serve",
    "build": "ng build --prod",
    "test": "ng test ngx-onboarding --watch=false --browsers=ChromeHeadless --code-coverage",
    "test-app": "ng test --watch=false --browsers=ChromeHeadless --code-coverage",
    "e2e": "ng e2e",
    "clean-dist": "rimraf dist",
    "build-lib": "ng build --configuration production ngx-onboarding && copyfiles *.md LICENSE.txt onboarding-demo.png dist/ngx-onboarding",
    "npm-pack": "cd dist/ngx-onboarding && npm pack",
    "package": "npm run clean-dist && npm run build-lib && npm run npm-pack"
  },
  "dependencies": {
    "@angular/animations": "^13.1.1",
    "@angular/cdk": "^13.1.1",
    "@angular/common": "^13.1.1",
    "@angular/compiler": "^13.1.1",
    "@angular/core": "^13.1.1",
    "@angular/forms": "^13.1.1",
    "@angular/material": "^13.1.1",
    "@angular/platform-browser": "^13.1.1",
    "@angular/platform-browser-dynamic": "^13.1.1",
    "@angular/router": "^13.1.1",
    "core-js": "^3.20.2",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^6.5.3",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^13.1.2",
    "@angular-devkit/build-ng-packagr": "~0.1002.0",
    "@angular/cli": "^13.1.2",
    "@angular/compiler-cli": "^13.1.1",
    "@angular/language-service": "^13.1.1",
    "@types/jasmine": "^2.8.14",
    "@types/jasminewd2": "^2.0.6",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "copyfiles": "^2.1.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "^6.3.14",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "karma-junit-reporter": "^1.2.0",
    "ng-packagr": "^13.1.2",
    "protractor": "~7.0.0",
    "ts-node": "~5.0.1",
    "tslib": "^2.3.1",
    "typescript": "~4.5.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rosen-group/ngx-onboarding.git"
  }
}
