    "@angular/platform-browser": "^12.0.2",
    "@angular/platform-browser-dynamic": "^12.0.2",
    "@angular/router": "^12.0.2",
    "classlist.js": "^1.1.20150312",
    "core-js": "^3.3.2",
    "lodash-es": "^4.17.15",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^6.5.3",
    "web-animations-js": "^2.3.2",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^12.0.5",
