    "onboarding"
  ],
  "scripts": {
    "ng": "ng",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test ngx-onboarding",
    "test-app": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "clean-dist": "rimraf dist",
    "build-lib": "ng build --prod ngx-onboarding && copyfiles *.md LICENSE.txt onboarding-demo.png dist/ngx-onboarding",
    "npm-pack": "cd dist/ngx-onboarding && npm pack",
    "package": "npm run clean-dist && npm run build-lib && npm run npm-pack"
  },
